<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>

    <ui:with field='msg' type='com.silverpeas.mobile.client.apps.notifications.resources.NotificationsMessages'/>

    <g:HTMLPanel tag="li" ui:field="container" styleName="notify">
        <g:Anchor styleName="ui-btn ui-btn-icon-right ui-icon-carat-more" ui:field="notify" text="{msg.notifyContent}"/>
    </g:HTMLPanel>
</ui:UiBinder>