<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui">
  <g:HTMLPanel tag="li" ui:field="container" styleName="media">

    <a class="ui-btn ui-icon-carat-r">
      <span ui:field="name"></span>&nbsp;(<span ui:field="priority" class="task-priority"></span>)
      <br/><span ui:field="delegator"></span><span ui:field="endDate" class="endDate"></span>
      <br/><span ui:field="percentCompleted"></span><g:TextBox ui:field="range" styleName="task-percent-range"></g:TextBox>
    </a>
  </g:HTMLPanel>
</ui:UiBinder> 