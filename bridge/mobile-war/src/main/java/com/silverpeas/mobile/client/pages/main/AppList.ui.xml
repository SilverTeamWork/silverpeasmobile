<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui">

  <ui:with field='msg' type='com.silverpeas.mobile.client.resources.ApplicationMessages'/>

  <g:HTMLPanel>
    <ul id="menu-principal">
      <li id="btn-find-document"><g:Anchor ui:field="document"><ui:safehtml from="{msg.appDocuments}"/></g:Anchor></li>
      <li id="btn-find-media"><g:Anchor ui:field="media"><ui:safehtml from="{msg.appMedia}"/></g:Anchor></li>
      <li id="btn-find-contact"><g:Anchor ui:field="contact"><ui:safehtml from="{msg.appContact}"/></g:Anchor></li>
      <li id="btn-update-statut"><g:Anchor ui:field="statut"><ui:safehtml from="{msg.appStatut}"/></g:Anchor></li>
      <li id="btn-news"><g:Anchor ui:field="news"><ui:safehtml from="{msg.appActus}"/></g:Anchor></li>
    </ul>
  </g:HTMLPanel>
</ui:UiBinder> 